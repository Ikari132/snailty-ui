{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules/*",
    "api/*",
    "assets/*"
  ],
  "compilerOptions": {
    "sourceMap": true,
    "resolveJsonModule": true,
    "module": "ESNext",
    "baseUrl": "./",
    "typeRoots": [
      "./",
      "node_modules"
    ],
    "paths": {
      "@*": [
        "./src*"
      ]
    }
  }
}